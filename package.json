{
  "name": "react-portfolio-site",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.2.2",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "gh-pages": "^2.0.1",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-animations": "^1.0.0",
    "react-dom": "^16.5.2",
    "react-fontawesome": "^1.6.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.5",
    "react-scroll-to-component": "^1.0.2"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar    --include-path ./src/sass ./src/sass/main.scss ./src/main.css --include-path ./src/sass  -o",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/sass/main.scss ./src/sass/main.css --watch --recursive",
    "start-js": "react-scripts start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
