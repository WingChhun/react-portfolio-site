{
  "name": "react-portfolio-site",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://wingchhun.github.io/react-portfolio-site",
  "dependencies": {
    "@material-ui/core": "^3.2.2",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-animated-css": "^1.0.4",
    "react-animations": "^1.0.0",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.5",
    "react-scroll-to-component": "^1.0.2",
    "react-swipeable-views": "^0.13.0",
    "react-swipeable-views-utils": "^0.13.0",
    "validator": "^10.8.0"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test:jest": "jest --config ./jest.config.json",
    "test:watch": "npm run test:jest -- --watch",
    "build-css": "node-sass-chokidar    --include-path ./src/sass ./src/sass/main.scss ./src/main.css --include-path ./src/sass  -o",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/sass/main.scss ./src/sass/main.css --watch --recursive",
    "deploy": "gh-pages  -d build",
    "predeploy": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint": "^7.27.0",
    "eslint-plugin-react": "^7.23.2",
    "gh-pages": "^3.2.0",
    "husky": "^6.0.0",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "terser": "^3.14.1"
  }
}
